        <div class="row" style="padding-left:20px; padding-right:20px"> 
            <md-content>
                <h4 style="color:dodgerblue">Health</h4>
                <md-divider></md-divider>
                <md-divider></md-divider>
                <md-divider></md-divider>
                <table class="table table-striped table-compact table-bordered">
                    <tr>
                        <td></td>
                        <td style="text-align:center">
                            <strong>Value</strong>
                        </td>
                        <td style="text-align:center">
                            <strong>Reference</strong>
                        </td>
                    </tr>
                    <tr>
                        <td style="vertical-align:middle" colspan="3">
                            <strong>1. Proportion of children aged 0-5 years old who died:</strong>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            &emsp;&emsp;Number of children aged 0-5 years old who died
                        </td>
                        <td>
                            <input type="number" ng-pattern="/^[0-9]{1,7}$/" class="form-control" name="health_number_0_5_value" ng-model="data.health_number_0_5_value"/>
                        </td>
                        <td>
                            <textarea class="form-control" class="form-control" name="health_number_0_5_reference" ng-model="data.health_number_0_5_reference"></textarea>
                        </td>
                    </tr>
                    <tr>
