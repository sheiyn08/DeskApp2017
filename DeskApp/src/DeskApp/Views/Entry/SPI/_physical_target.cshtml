                </div>
                <div flex>
                    <input class="form-control" name="Construction Target" type="number" ng-model="data.phy_construction_target" ng-disabled="true" />
                </div>
                <div flex>
                    <input class="form-control" name="Construction Actual" type="number" ng-model="data.phy_construction_actual" ng-disabled="true" />
                </div>
            </div>
            <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_improvement_target == true">
                <div flex>
                    <md-checkbox ng-model="data.phy_has_improvement_target" aria-label="Checkbox 1" ng-disabled="true">
                    </md-checkbox>
                </div>
                <div flex>
                    Improvement/Upgrading
                </div>
                <div flex>
