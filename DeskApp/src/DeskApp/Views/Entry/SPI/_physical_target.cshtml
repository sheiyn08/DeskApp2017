@*////--------------------------- v4.0 new layout, aligning design to Geotagging ---------------------------------------////*@


@{Layout = null; }

<div class="panel panel-info">

    <div class="panel-heading">
        <h4>Physical Progress Information</h4>
    </div>

    <div class="panel-body">
        <md-content>
            <h4 style="color:dodgerblue">Original POW</h4>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <md-divider></md-divider>

            <div layout="row" layout-margin flex="100">
                <div class="col-md-4">
                    <label>Physical Status <small class="text-info">(as per Original POW)</small></label>
                </div>
                <div class="col-md-8">
                    <select name="physical_status_id" ng-model="data.physical_status_id" id="physical_status_id" class="form-control"
                            ng-disabled="true"
                            ng-options="option.Id as option.Name for option in physical_status_id_options">
                        <option value="">[Select Physical Status]</option>
                    </select>
                </div>
            </div>
            <div layout="row" layout-margin flex="100" ng-show="data.physical_status_id == 5">
                <div class="col-md-4">
                    <label>Sub Project ID of Replacement</label>
                </div>
                <div class="col-md-8">
                    <input ng-model="data.replaced_sub_project_id" ng-disabled="true" class="form-control" />
                </div>
            </div>
            <div layout="row" layout-margin flex="100" >
                <div class="col-md-4">
                    <label>Date Started <small class="text-info">(as per Original POW)</small></label>
                </div>
                <div class="col-md-8">
                    <md-datepicker name="Date_Started" ng-disabled="true" ng-model="dt_Date_Started"></md-datepicker>
                </div>
            </div>
            <div layout="row" layout-margin flex="100" ng-show="data.physical_status_id !=3">
                <div class="col-md-4">
                    <label>Physical Accomplishment</label>
                </div>
                <div class="col-md-8">
                    <input type="number" ng-model="data.Phy_Perc_To_Date" class="form-control" ng-disabled="true" />
                </div>
            </div>
            <div layout="row" layout-margin flex="100" ng-show="data.physical_status_id == 3 && data.physical_status_id != 4">
                <div class="col-md-4">
                    <label>Physical Accomplishment</label>
                </div>
                <div class="col-md-8">
                    Not Yet started
                </div>
            </div>
            <div layout="row" layout-margin flex="100" ng-show="data.physical_status_id == 4 && data.physical_status_id != 3">
                <div class="col-md-4">
                    <label>Physical Accomplishment</label>
                </div>
                <div class="col-md-8">
                    Waived
                </div>
            </div>
            <div layout="row" layout-margin flex="100">
                <div class="col-md-4">
                    <label>Target Date of Completion <small class="text-info">(as per Original POW)</small></label>
                </div>
                <div class="col-md-8">
                    <md-datepicker ng-disabled="true" ng-model="dt_PlannedDate_Completion" ></md-datepicker>
                </div>
            </div>
            <div layout="row" layout-margin flex="100">
                <div class="col-md-4">
                    <label>Date Completed <small class="text-info">(as per Original POW)</small></label>
                </div>
                <div class="col-md-8">
                    <md-datepicker ng-disabled="true" ng-model="dt_Date_of_Completion"></md-datepicker>
                </div>
            </div>
        </md-content>

        <br /><br />

        <md-content>
            <h4 style="color:dodgerblue">Variaton Order / SPOW</h4>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <md-divider></md-divider>
            
            <div layout="row" layout-margin flex="100">
                <div class="col-md-4">
                    <label>With Variation Order?</label>
                </div>
                <div class="col-md-8">
                    <md-checkbox ng-model="data.has_variation" aria-label="Checkbox 1" class="md-primary" ng-disabled="true">
                        {{data.has_variation == true ? "Yes" : ""}}
                    </md-checkbox>
                </div>
            </div>
            <div class="panel-body" ng-show="data.has_variation == true">
                <div layout="row" layout-margin flex="100">
                    <div class="col-md-4">
                        <label>To make SP Functional</label>
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.is_sp_functional" aria-label="Checkbox 1" class="md-primary" ng-disabled="true">
                            {{data.is_sp_functional == true ? "Yes" : ""}}
                        </md-checkbox>
                    </div>
                </div>
                <div layout="row" layout-margin flex="100">
                    <div class="col-md-4">
                        <label>For Enhancement of Functionality</label>
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.is_enhancement_functionality" aria-label="Checkbox 1" class="md-primary" ng-disabled="true">
                            {{data.is_enhancement_functionality == true ? "Yes" : ""}}
                        </md-checkbox>
                    </div>
                </div>

                @*------- SPI Multiple Variation Date ----------*@
                <div layout="row" layout-margin flex="100">
                    <div class="col-md-4">
                        <label>Variation Order Date Approved</label>
                    </div>
                    <div class="col-md-8">
                        <table>
                            <tbody>
                                <tr ng-repeat="item in spi_multiple_variation_date">
                                    <td>
                                        Date Approved: {{item.date_approved | date : "longDate"}}
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div layout="row" layout-margin flex="100">
                    <div class="col-md-4">
                        <label>Physical Status <small class="text-info">(as per Variation Order)</small></label>
                    </div>
                    <div class="col-md-8">
                        <select name="variation_physical_status_id" ng-model="data.variation_physical_status_id" id="physical_status_id" class="form-control" ng-disabled="true"
                                ng-options="option.Id as option.Name for option in physical_status_id_options">
                            <option value="">[Select Physical Status]</option>
                        </select>
                    </div>
                </div>
                <div layout="row" layout-margin flex="100" ng-show="data.variation_physical_status_id !=3 && data.variation_physical_status_id !=4">
                    <div class="col-md-4">
                        <label>Physical Accomplishment </label>
                    </div>
                    <div class="col-md-8">
                        <input type="number" ng-model="data.variation_phy_perc_to_date" name="variation_phy_perc_to_date" class="form-control" ng-disabled="true" />
                    </div>
                </div>
                <div layout="row" layout-margin flex="100" ng-show="data.variation_physical_status_id == 3 && data.variation_physical_status_id != 4">
                    <div class="col-md-4">
                        <label>Physical Accomplishment </label>
                    </div>
                    <div class="col-md-8">
                        Not Yet Started
                    </div>
                </div>
                <div layout="row" layout-margin flex="100" ng-show="data.variation_physical_status_id == 4 && data.variation_physical_status_id != 3">
                    <div class="col-md-4">
                        <label>Physical Accomplishment </label>
                    </div>
                    <div class="col-md-8">
                        Waived
                    </div>
                </div>
                <div layout="row" layout-margin flex="100">
                    <div class="col-md-4">
                        <label>Target Date of Completion <small class="text-info">(as per Variation Order)</small></label>
                    </div>
                    <div class="col-md-8">
                        <md-datepicker ng-model="dt_variation_target_date_completion" ng-disabled="true"></md-datepicker>
                    </div>
                </div>
                <div layout="row" layout-margin flex="100">
                    <div class="col-md-4">
                        <label>Date Completed <small class="text-info">(as per Variation Order)</small></label>
                    </div>
                    <div class="col-md-8">
                        <md-datepicker ng-model="dt_variation_actual_date_completion" ng-disabled="true"></md-datepicker>
                    </div>
                </div>
            </div>
        </md-content>

        <br /><br />

        <md-content ng-show="data.modality_id != 11">
            <h4 style="color:dodgerblue">Physical Targets</h4>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <md-divider></md-divider>

            <div layout="row" layout-margin flex="100">
