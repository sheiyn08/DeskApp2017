
@*@{Layout = null; }

<div class="panel panel-info">

    <div class="panel-heading">
        <h4>Physical Progress</h4>
    </div>

    <div layout="row" layout-margin flex="100">
        <div class="col-md-4">
            <label for="Region:_">  With Tariff?</label>
        </div>
        <div class="col-md-8">
            <md-radio-group name="With Tariff" ng-model="data.with_tariff">
                <md-radio-button data-ng-value="true" class="md-primary" ng-disabled="true">Yes</md-radio-button>
                <md-radio-button data-ng-value="false" ng-disabled="true"> No </md-radio-button>
            </md-radio-group>
        </div>
    </div>

    <div layout="row" layout-margin flex="100">
        <div class="col-md-4">
            <label for="Region:_">Physical Status</label>
        </div>
        <div class="col-md-8">
            <select name="physical_status_id" ng-model="data.physical_status_id" id="physical_status_id" class="form-control"
                    ng-disabled="true"
                    ng-options="option.Id as option.Name for option in physical_status_id_options">
                <option value="">[Select Physical Status]</option>
            </select>
        </div>
    </div>

    <div layout="row" layout-margin flex="100">
        <div class="col-md-4">
            <label for="Region:_">Physical Accomplishment</label>
        </div>
        <div class="col-md-8">
            <input type="number" min="0" max="maxProgress" ng-model="data.Phy_Perc_To_Date" name="Phy_Perc_To_Date" class="form-control" ng-disabled="true" />
        </div>
    </div>

    <div layout="row" layout-margin flex="100">
        <div class="col-md-4">
            <label for="Region:_">Dates</label>
        </div>
        <div class="col-md-4">
            <label>Target</label>      
        </div>
        <div class="col-md-4">
            <label>Actual</label>      
        </div>
    </div>

    <div layout="row" layout-margin flex="100">
        <div class="col-md-4">
            <label for="Region:_">Date Started / Financial (First RFR date download)</label>
        </div>
        <div class="col-md-4">
        </div>
        <div class="col-md-4">
            
            <md-datepicker name="Date_Started" ng-disabled="true" ng-model="dt_Tranche1date"></md-datepicker>
            <div ng-messages="myForm.Date_Started.$error">
                <div ng-message="required" class="error">*Date Started is required for Completed Projects</div>
            </div>
        </div>
    </div>

    <div layout="row" layout-margin flex="100">
        <div class="col-md-4">
            <label for="Region:_">Date Started / Physical </label>
        </div>
        <div class="col-md-4">
           
            <md-datepicker name="Physical Date Start Target" ng-disabled="true" ng-model="dt_date_start_physical_planned"></md-datepicker>
        </div>
        <div class="col-md-4">
            
            <md-datepicker name="Physical Date Start Actual" ng-disabled="true" ng-model="dt_Date_Started"></md-datepicker>
            <div ng-messages="myForm.date_physical_start.$error">
                <div ng-message="required" class="error">*Date Started is required for Completed Projects</div>
            </div>
        </div>
    </div>  
    
    <div layout="row" layout-margin flex="100">
        <div class="col-md-4">
            <label for="Region:_">  Date of Completion</label>
        </div>
        <div class="col-md-4">
            <md-datepicker name="Date of Completion Target" ng-disabled="true" ng-model="dt_PlannedDate_Completion"></md-datepicker>
            
        </div>
        <div class="col-md-4">
            <md-datepicker name="Date of Completion Actual" ng-disabled="true" ng-model="dt_Date_of_Completion"></md-datepicker>
            
        </div>
    </div>

</div>

<div class="panel panel-info">

    <div class="panel-heading">
        <h4>Physical Target and Actual Accomplishment</h4>
    </div>

    <div layout="row" layout-margin flex="100">
        <div flex>Active? <small>Check if YES</small></div>
        <div flex>Category</div>
        <div flex>Unit of Measure</div>
        <div flex>Target</div>
        <div flex>Actual (if completed)</div>
    </div>

    <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_construction_target == true">
        <div flex>
            <md-checkbox ng-model="data.phy_has_construction_target" aria-label="Checkbox 1" ng-disabled="true">
            </md-checkbox>
        </div>
        <div flex>
            Construction
        </div>
        <div flex>
            {{lib_project_type.phy_construction_unit}}
        </div>
        <div flex>
            
            <input class="form-control" name="Construction Target" type="number" ng-model="data.phy_construction_target" ng-change="data.s_phy_construction_target = data.phy_construction_target" ng-disabled="true" />
        </div>
        <div flex>
            
            <input class="form-control" name="Construction Actual" type="number" ng-model="data.phy_construction_actual" ng-change="data.s_phy_construction_actual = data.phy_construction_actual" ng-disabled="true" />
        </div>
    </div>

    <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_improvement_target ==true">
        <div flex>
            <md-checkbox ng-model="data.phy_has_improvement_target" aria-label="Checkbox 1" ng-disabled="true">
            </md-checkbox>
        </div>
        <div flex>
            Improvement/Upgrading
        </div>
        <div flex>
            {{lib_project_type.phy_improvement_unit}}
        </div>
        <div flex>
            
            <input class="form-control" name="Improvement Target" type="number" ng-model="data.phy_improvement_target" ng-change="data.s_phy_improvement_target = data.phy_improvement_target" ng-disabled="true" />
        </div>
        <div flex>
            
            <input class="form-control" name="Improvement Actual" type="number" ng-model="data.phy_improvement_actual" ng-change="data.s_phy_improvement_actual = data.phy_improvement_actual" ng-disabled="true" />
        </div>
    </div>

    <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_rehabilitation_target">
        <div flex>
            <md-checkbox ng-model="data.phy_has_rehabilitation_target" aria-label="Checkbox 1" ng-disabled="true">
            </md-checkbox>
        </div>
        <div flex>
            Rehabilitation /Repair / Reconstruction
        </div>
        <div flex>
            {{lib_project_type.phy_rehabilitation_unit}}
        </div>
        <div flex>
            
            <input class="form-control" name="Rehab Target" type="number" ng-model="data.phy_rehabilitation_target" ng-change="data.s_phy_rehabilitation_target = data.phy_rehabilitation_target" ng-disabled="true" />
        </div>
        <div flex>
            
            <input class="form-control" name="Rehab Actual" type="number" ng-model="data.phy_rehabilitation_actual" ng-change="data.s_phy_rehabilitation_actual = data.phy_rehabilitation_actual" ng-disabled="true" />
        </div>
    </div>

    <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_purchase_target">
        <div flex>
            <md-checkbox ng-model="data.phy_has_purchase_target" aria-label="Checkbox 1">
            </md-checkbox>
        </div>
        <div flex>
            Purchase / Acquisition / Provision (training)
        </div>
        <div flex>
            {{lib_project_type.phy_purchase_unit}}
        </div>
        <div flex>
            
            <input class="form-control" type="number" ng-model="data.phy_purchase_target" ng-change="data.s_phy_purchase_target = data.phy_purchase_target" ng-disabled="true"/>
        </div>
        <div flex>
            
            <input class="form-control" type="number" ng-model="data.phy_purchase_actual" ng-change="data.s_phy_purchase_actual = data.phy_purchase_actual" ng-disabled="true"/>
        </div>
    </div>
    

    <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_construction_target_secondary == true">
        <div flex>
            <md-checkbox ng-model="data.phy_has_construction_target_secondary" aria-label="Checkbox 1" ng-disabled="true">
            </md-checkbox>
        </div>
        <div flex>
            Construction
        </div>
        <div flex>
            {{lib_project_type.phy_construction_unit_secondary}}
        </div>
        <div flex>
            
            <input class="form-control" type="number" ng-model="data.phy_construction_target_secondary" ng-change="data.s_phy_construction_target_secondary = data.phy_construction_target_secondary"  />
        </div>
        <div flex>
            
            <input class="form-control" type="number" ng-model="data.phy_construction_actual_secondary" ng-change="data.s_phy_construction_actual_secondary = data.phy_construction_actual_secondary"  />
        </div>
    </div>

    <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_improvement_target_secondary ==true">
        <div flex>
            <md-checkbox ng-model="data.phy_has_improvement_target_secondary" aria-label="Checkbox 1" ng-disabled="true">
            </md-checkbox>
        </div>
        <div flex>
            Improvement/Upgrading
        </div>
        <div flex>
            {{lib_project_type.phy_improvement_unit_secondary}}
        </div>
        <div flex>
            
            <input class="form-control" type="number" ng-model="data.phy_improvement_target_secondary" ng-change="data.s_phy_improvement_target_secondary = data.phy_improvement_target_secondary"  />
        </div>
        <div flex>
            
            <input class="form-control" type="number" ng-model="data.phy_improvement_actual_secondary" ng-change="data.s_phy_improvement_actual_secondary = data.phy_improvement_actual_secondary" />
        </div>
    </div>

    <div layout="row" layout-margin flex="100" ng-show="lib_project_type.phy_has_rehabilitation_target_secondary">
