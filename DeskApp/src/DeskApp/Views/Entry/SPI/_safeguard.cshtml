                <div class="row">
                    <div class="col-md-4">
                        Submitted request for validation to NCIP
                    </div>
                    <div class="col-md-1">
                        <md-checkbox ng-model="data.ncip_submitted" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                    <div class="col-md-4">
                        <md-datepicker name="Date Provided" ng-disabled="true" ng-model="dt_ncip_submitted_date"></md-datepicker>
                        @*<md-datepicker name="Date Provided" ng-disabled="!data.ncip_submitted" ng-required="data.ncip_submitted" ng-model="dt_ncip_submitted_date" ng-change="data.ncip_submitted_date = dt_ncip_submitted_date.toISOString()" md-placeholder="Enter date" md-max-date="today"></md-datepicker>*@
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        Validation Conducted
                    </div>
                    <div class="col-md-1">
                        <md-checkbox ng-model="data.ncip_validated" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                    <div class="col-md-4">
                        <md-datepicker name="Date Provided" ng-disabled="true" ng-model="dt_ncip_validated_date"></md-datepicker>
                        @*<md-datepicker md-min-date="dt_ncip_submitted_date" name="Date Provided" ng-disabled="!data.ncip_validated" ng-required="data.ncip_validated" ng-model="dt_ncip_validated_date" ng-change="data.ncip_validated_date = dt_ncip_validated_date.toISOString()" md-placeholder="Enter date" md-max-date="today"></md-datepicker>*@
