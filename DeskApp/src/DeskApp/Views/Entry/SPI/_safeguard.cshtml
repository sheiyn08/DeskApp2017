            <md-divider></md-divider>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <div class="panel panel-body">
                <div class="row">
                    <div class="col-md-4">
                        With ESSC?
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.has_sc_essc" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        With ESMP?
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.has_sc_esmp" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        With ECC?
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.has_sc_ecc" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        With CNC?
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.has_sc_cnc" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
                @*<div class="row">
                <div class="col-md-4">
                    With CP?
                </div>
                <div class="col-md-8">
                    <md-checkbox ng-model="data.has_sc_cp" aria-label="Checkbox 1">
                    </md-checkbox>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    With CNO?
                </div>
                <div class="col-md-8">
                    <md-checkbox ng-model="data.has_sc_cno" aria-label="Checkbox 1">
                    </md-checkbox>
                </div>
            </div>*@
            </div>
            
            <h4 style="color:dodgerblue">Indigenous Peoples</h4>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <div class="panel panel-body">
                <div class="row">
                    <div class="col-md-4">
                        With IP presence in the Barangay?
                    </div>
                    <div class="col-md-8">
                        <md-radio-group ng-required="true" name="With IP Presence in the Barangay" ng-model="data.has_ip_presence">
                            <md-radio-button data-ng-value="true" class="md-primary" ng-disabled="true">Yes</md-radio-button>
                            <md-radio-button data-ng-value="false" ng-disabled="true"> No </md-radio-button>
                        </md-radio-group>
                    </div>
                </div>
            </div>

            <h4 style="color:dodgerblue">Ancestral Domain Status</h4>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <div class="panel panel-body">
                <div class="row">
                    <div class="col-md-4">
                        Approved CADT
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.with_cadt" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        On Process
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.on_process_cadt" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        CADTeable
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.cadteable" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
            </div>

            <h4 style="color:dodgerblue">Validation Status</h4>
            <md-divider></md-divider>
            <md-divider></md-divider>
            <md-divider></md-divider>
