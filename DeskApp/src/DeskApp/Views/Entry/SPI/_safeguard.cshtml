                    <div class="col-md-4">
                        With Certification Pre-condition
                    </div>
                    <div class="col-md-8">
                        <md-checkbox ng-model="data.with_cert_precondition" aria-label="Checkbox 1" ng-disabled="true">
                        </md-checkbox>
                    </div>
                </div>
