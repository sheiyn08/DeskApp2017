                                        <input class="form-control" required="required" readonly="readonly" name="psgc_id" type="text" id="psgc_id">
                                    </div>

                                    <div class="form-group hidden">
                                        <label for="kc_code">KC-NCDDP Code</label>
                                        <input class="form-control" name="kc_code" type="text" id="kc_code">
                                    </div>

                                    

                                  
                                    <div class="form-group">
                                        <label for="">Cycle <i class="text-red">*</i></label>
                                        <select class="form-control" required="required" name="cycle_id"><option value="" selected="selected">Select Cycle</option><option value="NULL">Select Cycle</option><option value="1">1</option><option value="2">2</option><option value="3">3</option><option value="GuarC1">GuarC1</option><option value="GuarC2">GuarC2</option><option value="MT">MT</option><option value="NCDDP-1">NCDDP-1</option><option value="NCDDP-2">NCDDP-2</option><option value="NCDDP-3">NCDDP-3</option><option value="NCDDP-4">NCDDP-4</option><option value="RandomC1">RandomC1</option><option value="RandomC2">RandomC2</option><option value="RandomC3">RandomC3</option></select>
                                    </div>


                                    <div class="form-group">
                                        <label for="program_id">Program</label>
                                        <select class="form-control" required="required" id="program_id" name="program_id"><option value="" selected="selected">Select Program</option><option value="Australian DFAT">Australian DFAT</option><option value="GIG">GIG</option><option value="JFPR">JFPR</option><option value="JSDF">JSDF</option><option value="KC-GBP">KC-GBP</option><option value="KC-MCC">KC-MCC</option><option value="KC-NCDDP">KC-NCDDP</option><option value="Others">Others</option><option value="PAMANA">PAMANA</option></select>
