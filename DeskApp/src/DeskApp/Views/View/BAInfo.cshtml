                                    <div class="form-group">
                                        <label for="">Barangay <i class="text-red">*</i></label>
                                        <select class="form-control" required="required" id="barangay" name="barangay"><option value="NULL">Select Barangay</option><option value="144401001">Chupac</option><option value="144401002">Fiangtin</option><option value="144401003">Kaleo</option><option value="144401005">Latang</option><option value="144401006">Lias Kanluran</option><option value="144401007">Lingoy</option><option value="144401008">Lunas</option><option value="144401009">Macalana</option><option value="144401010">Ogoog</option><option value="144401011">Gawana (Pob.)</option><option value="144401012">Lias Silangan</option></select>
